<GridPane alignment="center" hgap="975.0" vgap="720.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <AnchorPane prefHeight="720.0" prefWidth="975.0">
         <children>
            <ImageView blendMode="MULTIPLY" fitHeight="720.0" fitWidth="975.0">
               <image>
                  <Image url="@../images/Contact_IMG.png" />
               </image>
            </ImageView>
            <TableView fx:id="table_view" blendMode="MULTIPLY" layoutX="611.0" layoutY="91.0" prefHeight="572.0" prefWidth="330.0">
              <columns>
                <TableColumn fx:id="Name" prefWidth="111.0" text="Name" />
                <TableColumn fx:id="Mobile" prefWidth="100.0" text="Mobile" />
                  <TableColumn fx:id="Email" prefWidth="118.0" text="Email" />
              </columns>
            </TableView>
            <Button fx:id="add" blendMode="MULTIPLY" layoutX="265.0" layoutY="155.0" mnemonicParsing="false" onAction="#add_action" prefHeight="67.0" prefWidth="77.0" />
            <Button fx:id="back" blendMode="MULTIPLY" layoutX="829.0" layoutY="17.0" mnemonicParsing="false" onAction="#back_action" prefHeight="44.0" prefWidth="122.0" />
            <Button fx:id="delete" blendMode="MULTIPLY" layoutX="504.0" layoutY="287.0" mnemonicParsing="false" onAction="#delete_action" prefHeight="25.0" prefWidth="52.0" />
         </children>
      </AnchorPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
